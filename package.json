{
  "name": "image-browser",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "validate": "tsx src/shared/validate-env.ts",
    "upload": "tsx src/syncer/upload.ts",
    "fast-upload": "SKIP_R2_HEAD=true tsx src/syncer/upload.ts",
    "embed": "tsx src/syncer/embed.ts",
    "sync": "tsx src/syncer/sync.ts",
    "backfill-dimensions": "tsx src/syncer/backfill-dimensions.ts",
    "ensure-schema": "tsx src/shared/db.ts ensure-schema",
    "server": "tsx src/server/server.ts",
    "server:dev": "nodemon --watch src --ext ts --exec \"tsx src/server/server.ts\"",
    "server:build": "tsc -p .",
    "server:start": "node dist/server/server.js",
    "build": "tsc -p .",
    "start": "node dist/server/server.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.614.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "image-size": "1.1.1",
    "mime": "^3.0.0",
    "p-limit": "^5.0.0",
    "pg": "^8.12.0",
    "replicate": "^0.31.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.8.4",
    "@types/pg": "^8.11.6",
    "@types/mime": "^3.0.4",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.0.0"
}
