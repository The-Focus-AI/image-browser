SUPABASE_DB_URL=postgresql://user:pass@host:5432/dbname
REPLICATE_API_TOKEN=your_replicate_token_here

# Path (relative to browse/) where local images live for now
IMAGES_DIR=../images

# Optional: set this when moving to Cloudflare R2 (public base URL)
IMAGE_BASE_URL=https://bucket.accountid.r2.cloudflarestorage.com

# Replicate text embedding model and input key (required for text search)
# Defaults to krthr/clip-embeddings which returns { embedding: number[] }
# REPLICATE_TEXT_MODEL=krthr/clip-embeddings:1c0371070cb827ec3c7f2f28adcdde54b50dcd239aa6faea0bc98b174ef03fb4
# REPLICATE_TEXT_INPUT_KEY=text

# Server port
PORT=3000

# Enforce embedding vector dimension to avoid DB errors
EXPECTED_VECTOR_DIM=768


